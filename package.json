{
  "name": "pe-toolkit",
  "description": "A tookit for working with Windows PE exe, dll, or some msi files",
  "version": "0.2.0",
  "author": "ryanc16",
  "license": "ISC",
  "main": "lib/index",
  "files": [
    "lib/"
  ],
  "types": "lib/index",
  "typings": "lib/index",
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run gen:docs",
    "build": "tsc",
    "test": "jasmine --config=jasmine.json",
    "example": "node examples/get-exe-info.js",
    "gen:docs": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryanc16/pe-toolkit.git"
  },
  "keywords": [
    "pe file",
    "vs version info",
    "properties",
    "windows",
    "exe",
    "msi",
    "dll"
  ],
  "bugs": {
    "url": "https://github.com/ryanc16/pe-toolkit/issues"
  },
  "homepage": "https://github.com/ryanc16/pe-toolkit#readme",
  "devDependencies": {
    "@types/node": "^18.11.17",
    "jasmine": "^3.10.0",
    "jasmine-console-reporter": "^3.1.0",
    "jasmine-spec-reporter": "^7.0.0",
    "typedoc": "^0.23.23",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.9.4"
  }
}
